{
  "name": "hcc/wp-development-core-library",
  "description": "WordPress development framework with reusable components",
  "version": "1.0.0",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "composer/installers": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "HCC\\Core": "src/"
    }
  },
  "extra": {
    "installer-name": "wp-development-core-library"
  },
  "config": {
    "vendor-dir": "lib"
  }
}