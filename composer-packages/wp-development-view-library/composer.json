{
  "name": "hcc/wp-development-view-library",
  "description": "WordPress plugin development library for MVC view presentation",
  "version": "1.0.0",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "composer/installers": "^1.10",
    "hcc/wp-development-core-library": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "HCC\\View": "src/"
    }
  },
  "extra": {
    "installer-name": "wp-development-view-library"
  },
  "config": {
    "vendor-dir": "lib"
  }
}